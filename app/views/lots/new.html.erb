<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new lot</h1>
  <%= simple_form_for @lot, url: lots_path do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>